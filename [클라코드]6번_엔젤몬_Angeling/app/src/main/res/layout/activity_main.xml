<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:openDrawer="start">

    <include
        layout="@layout/app_bar"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <android.support.design.widget.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="165dp"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:background="#ffffff"
        android:fitsSystemWindows="true">
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="vertical"
                android:background="#ffffff"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:background="#ffffff">
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:gravity="center"
                        android:background="#cc000000">
                        <ImageView
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_marginRight="5dp"
                            android:src="@mipmap/left_wing"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="테마별 봉사"
                            android:id="@+id/TextView_Thema_Title"
                            android:textSize="18dp"
                            android:textColor="#ffffff"/>
                        <ImageView
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_marginLeft="5dp"
                            android:src="@mipmap/right_wing"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginTop="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="42dp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/env"
                        android:background="#ffffff">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="환경 봉사"
                            android:textColor="#000000"
                            android:id="@+id/TextView_Thema_env"
                            android:textSize="16.5dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#ffffff"
                        android:gravity="center">
                        <ImageView
                            android:layout_marginTop="3.5dp"
                            android:layout_marginBottom="3.5dp"
                            android:layout_width="107dp"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/drawer_line"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="42dp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/edu"
                        android:background="#ffffff">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="지식나눔 봉사"
                            android:textColor="#000000"
                            android:id="@+id/TextView_Thema_edu"
                            android:textSize="16.5dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#ffffff"
                        android:gravity="center">
                        <ImageView
                            android:layout_marginTop="3.5dp"
                            android:layout_marginBottom="3.5dp"
                            android:layout_width="107dp"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/drawer_line"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="42dp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/local"
                        android:background="#ffffff">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="지역센터 봉사"
                            android:textColor="#000000"
                            android:id="@+id/TextView_Thema_local"
                            android:textSize="16.5dp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#ffffff"
                        android:gravity="center">
                        <ImageView
                            android:layout_marginTop="3.5dp"
                            android:layout_marginBottom="3.5dp"
                            android:layout_width="107dp"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/drawer_line"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="42dp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/ali"
                        android:background="#ffffff">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="소외계층 봉사"
                            android:textColor="#000000"
                            android:id="@+id/TextView_Thema_ali"
                            android:textSize="16.5dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#ffffff"
                        android:gravity="center">
                        <ImageView
                            android:layout_marginTop="3.5dp"
                            android:layout_marginBottom="3.5dp"
                            android:layout_width="107dp"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/drawer_line"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="42dp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/living"
                        android:background="#ffffff">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="생활편의 봉사"
                            android:textColor="#000000"
                            android:id="@+id/TextView_Thema_living"
                            android:textSize="16.5dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#ffffff"
                        android:gravity="center">
                        <ImageView
                            android:layout_marginTop="3.5dp"
                            android:layout_marginBottom="3.5dp"
                            android:layout_width="107dp"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/drawer_line"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="42dp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/other"
                        android:background="#ffffff">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="기타 봉사"
                            android:textColor="#000000"
                            android:id="@+id/TextView_Thema_other"
                            android:textSize="16.5dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#888888"
                        android:gravity="center">
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:gravity="center"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:background="#cc000000">
                        <ImageView
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_marginRight="5dp"
                            android:src="@mipmap/left_wing"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="지역별 봉사"
                            android:id="@+id/TextView_Region_Title"
                            android:textSize="18dp"
                            android:textColor="#ffffff"/>
                        <ImageView
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_marginLeft="5dp"
                            android:src="@mipmap/right_wing"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="42dp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/seoul"
                        android:background="#ffffff">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="서울"
                            android:textColor="#000000"
                            android:id="@+id/TextView_Region_seoul"
                            android:textSize="16.5dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#ffffff"
                        android:gravity="center">
                        <ImageView
                            android:layout_marginTop="3.5dp"
                            android:layout_marginBottom="3.5dp"
                            android:layout_width="107dp"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/drawer_line"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="42dp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/gyeonggi"
                        android:background="#ffffff">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="경기"
                            android:textColor="#000000"
                            android:id="@+id/TextView_Region_gyeonggi"
                            android:textSize="16.5dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#ffffff"
                        android:gravity="center">
                        <ImageView
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:layout_width="107dp"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/drawer_line"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="42dp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/busan"
                        android:background="#ffffff">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="부산"
                            android:textColor="#000000"
                            android:id="@+id/TextView_Region_busan"
                            android:textSize="16.5dp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#ffffff"
                        android:gravity="center">
                        <ImageView
                            android:layout_marginTop="3.5dp"
                            android:layout_marginBottom="3.5dp"
                            android:layout_width="107dp"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/drawer_line"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="42dp"
                        android:gravity="center"
                        android:id="@+id/region_other"
                        android:background="#ffffff">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="기타"
                            android:textColor="#000000"
                            android:id="@+id/TextView_Region_other"
                            android:textSize="16.5dp" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>
        </ScrollView>
    </android.support.design.widget.NavigationView>

</android.support.v4.widget.DrawerLayout>